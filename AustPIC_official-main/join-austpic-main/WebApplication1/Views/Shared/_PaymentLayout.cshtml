    <title>Join AUSTPIC</title>
    <meta content="Join the Rapidly Growing Club of AUST | AUSTPIC" name="description">
    <meta content="join,austpic,aust" name="keywords">

    <!-- Favicons -->
    <link href="~/img/common/club_logo_small.png" rel="icon">
    <link href="~/img/common/club_logo_medium.png" rel="apple-touch-icon">

